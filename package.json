{
  "name": "gitlab-2-github",
  "version": "0.1.5",
  "description": "Migrate Issues, Wiki from gitlab to github",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "start": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piceaTech/node-gitlab-2-github.git"
  },
  "keywords": [
    "github",
    "gitlab",
    "import",
    "export",
    "issue",
    "issues"
  ],
  "author": "Spruce, <dev@spruce.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piceaTech/node-gitlab-2-github/issues"
  },
  "homepage": "https://github.com/piceaTech/node-gitlab-2-github#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@gitbeaker/node": "^35.1.0",
    "@octokit/plugin-throttling": "^11.0.1",
    "@octokit/rest": "^21.1.1",
    "@octokit/types": "^14.1.0",
    "aws-sdk": "^2.1053.0",
    "axios": "^1.6.0",
    "mime-types": "^2.1.34",
    "readline-sync": "^1.4.10",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.1",
    "@types/node": "^14.18.5",
    "@types/readline-sync": "^1.4.4",
    "husky": "^7.0.4",
    "lint-staged": "^13.2.1",
    "prettier": "^2.5.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^5.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
